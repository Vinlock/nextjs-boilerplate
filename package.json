{
  "name": "nextjs-standalone-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "up": "docker compose up -d --remove-orphans",
    "docker-dev": "yarn && kill-port 9229 && NODE_OPTIONS='--inspect 0.0.0.0' yarn dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dc": "docker compose",
    "prisma": "npx prisma",
    "migrate": "yarn prisma migrate",
    "migrate-dev": "yarn migrate dev",
    "migrate-prod": "yarn migrate deploy",
    "studio": "npx prisma studio",
    "generate": "npx prisma generate",
    "vercel:build": "next build && DATABASE_URL=$MIGRATE_DATABASE_URL npx prisma migrate deploy"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.9.1",
    "@aws-sdk/client-s3": "^3.142.0",
    "@aws-sdk/s3-request-presigner": "^3.142.0",
    "@fortawesome/fontawesome-free": "6.1.1",
    "@fortawesome/fontawesome-pro": "6.1.1",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/pro-duotone-svg-icons": "6.1.1",
    "@fortawesome/pro-light-svg-icons": "6.1.1",
    "@fortawesome/pro-regular-svg-icons": "6.1.1",
    "@fortawesome/pro-solid-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.1.18",
    "@mux/mux-node": "^6.2.0",
    "@mux/mux-video-react": "^0.5.1",
    "@mux/upchunk": "2.3.1",
    "@prisma/client": "4.2.1",
    "@tanstack/react-query": "^4.2.3",
    "@types/google.maps": "^3.49.2",
    "auth0-js": "^9.19.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "cookies": "^0.8.0",
    "formik": "^2.2.9",
    "google-places-web": "^2.0.1",
    "i18next": "^21.9.1",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "next": "12.2.3",
    "next-connect": "^0.13.0",
    "next-joi": "^2.2.1",
    "next-superjson": "^0.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.2",
    "react-input-mask": "2.0.4",
    "superjson": "^1.9.1",
    "usehooks-ts": "^2.6.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/cookies": "^0.7.7",
    "@types/node": "18.6.2",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-input-mask": "2.0.5",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.20.0",
    "eslint-config-next": "12.2.3",
    "kill-port": "^2.0.1",
    "postcss": "^8.4.14",
    "prisma": "4.2.1",
    "tailwindcss": "3.1.6",
    "typescript": "^4.7.4"
  }
